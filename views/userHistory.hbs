<div class="row">
    <div class="col-md-12">
    <h2>Web322 user {{session.user.userName}} ( {{session.user.email}} ) History</h2>
    <hr />
    <div class="table-responsive">
    <table class="table">
        <thead>
            <tr>
            <th>Login Date/Time</th>
            <th>Client Information</th>
        </tr>
        </thead>
        {{#if session.user.loginHistory}}
            {{#each session.user.loginHistory}}
            <tr>
                <td>{{dateTime}}</td>
                <td>{{userAgent}}</td>
            </tr>
            {{/each}}
        {{/if}}
    </table>
    </div>
    </div>
</div>